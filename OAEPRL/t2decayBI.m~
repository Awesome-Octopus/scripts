function f = t2decayBI(x,name);

[b spec params] = eprload(name);
[shift ns] = strread(params.FTEzDelay1,'%f %s');
[b] = real(b);
[spec] = real(spec);
b = b + shift;
b = b';
spec = spec';
[xsim] = linspace(min(b),max(b),params.XPTS);
f = sum((abs(spec-((x(3)*exp(-(1/x(4)).*xsim)+(x(1)*exp(-(1/x(2)).*xsim))))).^2/params.XPTS).^0.5);
