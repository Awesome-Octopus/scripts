clear all
[x y params] = eprload()
yreal = real(y)
yimag = imag(y)

if (sum(y) == 0)
xlabel('Field (G)','FontSize',14), ylabel('d\chi"/dB','FontSize',14);
title('Normalized Time Domain','FontSize',16);
data = [x' yreal]
fname = input('Choose a File Name: ','s')
fnamecw = sprintf('%s.txt',fname);
save(fnamecw,'data','-ascii')
else
    plot(x,yreal,x,yimag)
    data = [x yreal yimag]
    fnamepulse = sprintf('%s.txt',params.TITL)
    save(fnamepulse,'data','-ascii')
end