clear all

%% Creates a matrix of spins along the z axis

for i=1:50
    Spins.degrees(i,:) = [0,90,1];
end

%% Sets the evolution parameters and function between pulses

Spins.Tone = 10000;
Spins.Ttwo = 10000;
Spins.offset = linspace(51/60,65/60,50);

Sim.length = 180;
Sim.inc = 0.1;
Sim.delay = 0.1;

Spins.degrees = evolution(Spins,Sim);