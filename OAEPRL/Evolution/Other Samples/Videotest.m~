clear all

%% Creates a matrix of spins along the z axis

for i=1:250
    Spins.degrees(i,:) = [0,90,1];
end

Spins.Video = 'y'

Spins.Tone = 0.000219;
Spins.Ttwo = 0.0000115;
Spins.offset = linspace(-1600000,1600000,250);

Sim.length = 0.0000005;
Sim.inc = 0.00000001;
Sim.delay = 0.05;

[Spins.degrees Spins.frame] = evolution(Spins,Sim);

[Spins.degrees Spins.frame] = evolution(Spins,Sim);