clear all
[b spec params] = eprload('ACHR_V12_i+4_11-2-2009_3P120.DTA');
[shift ns] = strread(params.FTEzDelay1,'%f %s');
[shift2 ns2] = strread(params.FTEzDelay2,'%f %s');
b = (b' + shift + shift2);
spec = real(spec);
[k,c,yfit] = exponfit(b,spec,2);
scale = max(yfit);
spec_scaled = spec/max(scale);
yfit_scaled = yfit/max(scale);
ysub = spec-yfit;
subplot(2,1,1);
plot(b,spec_scaled,b,yfit_scaled);
xlabel('\tau+T(ns)','FontSize',18), ylabel('normalizedintensity','FontSize',18);
title('Normalized Time Domain','FontSize',20);
subplot(2,1,2);
plot(b,ysub);
data = [b' spec yfit spec_scaled yfit_scaled];
save