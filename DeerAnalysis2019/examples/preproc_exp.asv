tag='87C';
oname=[tag '_exp'];

data=load([tag '_bckg.dat']);
data2=load([tag '_distr.dat']);

texp=data(:,1);
[mini,minp]=min(abs(texp));
texp=texp(minp:length(texp));
vexp=data(:,2);
vexp=vexp(minp:length(vexp));

rax=data2(:,1);
distr=data2(:,2);

figure(1); clf;
plot(texp,vexp,'k');

figure(2); clf;
plot(rax,distr,'k');

save(oname,'texp','vexp','rax','distr');
